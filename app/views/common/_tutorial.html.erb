<div id="tutorial-slideshow">
    <ul class="bjqs">
		<% (1..7).each do |i| %>
		   <li>
			   <%= image_tag "Tour_%02i.png" % i %>
		   </li>
		<% end %>
    </ul>
</div>

<% content_for :stylesheets do 
    stylesheet_link_tag 'bjqs'
end %>

<% content_for :javascripts do 
	javascript_include_tag '_tutorial', 'bjqs', 'jquery.blockUI'
end %>